diff --git a/index.ts b/index.ts
new file mode 100644
index 0000000000000000000000000000000000000000..55f220e10afa8ffb90efad287380b11196af453e
--- /dev/null
+++ b/index.ts
@@ -0,0 +1,136 @@
+import {readdir, readFile, writeFile} from 'fs/promises'
+import {join, resolve} from 'path'
+
+main()
+
+async function main(): Promise<void> {
+  try {
+    const [, , relativePath] = process.argv
+    if (!relativePath) {
+      console.error('Please provide a path argument:')
+      console.error('Usage: node transform-files.js <relative_path>')
+      process.exit(1)
+    }
+
+    await transformFiles({
+      path: relativePath,
+      transform: transformMonacoEditor,
+    })
+  } catch (error) {
+    console.error('Error:', error)
+    process.exit(1)
+  }
+}
+
+/**
+ * Transforms Monaco Editor source files by modifying localize function calls to include the file path.
+ * This is specifically for files within the monaco-editor directory that contain localize function calls.
+ */
+async function transformMonacoEditor(
+  // The full path to the file being processed
+  filepath: string,
+  // The source content of the file
+  source: string,
+): Promise<string | null> {
+  if (filepath.includes('esm')) {
+    const regex = /esm\/(.+)\.js/
+    const match = filepath.match(regex)
+
+    if (match) {
+      const path = match[1]
+
+      if (source.includes('localize2(')) {
+        const result = source.replace(
+          /(\bfunction\s+localize2\()|(\blocalize2\()/g,
+          text => {
+            const isFunctionStatement = /function\s+localize/.test(text)
+
+            if (isFunctionStatement) {
+              console.log(`Function statement, skipping...`)
+
+              return text
+            }
+
+            console.log(`Replacing localize2( with localize2('${path}', ...)`)
+
+            return `localize2('${path}', `
+          },
+        )
+
+        return result
+      }
+    }
+  }
+
+  return null
+}
+
+/**
+ * Configuration options for the file transformer
+ */
+type TransformOptions = {
+  path: string
+  transform: (filepath: string, source: string) => Promise<string | null>
+}
+
+/**
+ * Transforms files in the specified directory using the provided transform function
+ */
+async function transformFiles(options: TransformOptions): Promise<void> {
+  try {
+    const {path: dirPath, transform} = options
+    const resolvedPath = resolve(dirPath)
+    const files = await getAllFiles(resolvedPath)
+
+    const transformations = files
+      .map(async filepath => {
+        try {
+          const source = await readFile(filepath, 'utf-8')
+          const result = await transform(filepath, source)
+
+          if (typeof result !== 'string') {
+            return null
+          }
+
+          await writeFile(filepath, result, 'utf-8')
+
+          return filepath
+        } catch (error) {
+          console.error(`Error processing file ${filepath}:`, error)
+          throw error
+        }
+      })
+      .filter(Boolean)
+
+    await Promise.all(transformations)
+
+    console.log(
+      `Successfully transformed ${files.length} files in ${resolvedPath}`,
+    )
+  } catch (error) {
+    console.error('Error during file transformation:', error)
+    throw error
+  }
+}
+
+/**
+ * Gets all files in a directory recursively
+ */
+async function getAllFiles(
+  dirPath: string,
+  fileList: string[] = [],
+): Promise<string[]> {
+  const files = await readdir(dirPath, {withFileTypes: true})
+
+  for (const file of files) {
+    const fullPath = join(dirPath, file.name)
+
+    if (file.isDirectory()) {
+      await getAllFiles(fullPath, fileList)
+    } else {
+      fileList.push(fullPath)
+    }
+  }
+
+  return fileList
+}
